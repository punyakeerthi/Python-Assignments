from mimetypes import init

class Account:
    owner=''
    balance=0
    
    def __init__(self,owner,balance):
        self.owner=owner
        self.balance=balance
        print("Account owner: "+owner)
        print("Account balance: "+balance)



acct1 = Account('Jose',100)
print(acct1)   
